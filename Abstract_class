abstract class Shape {
    abstract int area();
}

class Rectangle extends Shape {
public int width;
public int height;   
Rectangle(int width, int height) {
  this.width = width;
   this.height = height;
    }

 public int area() {
  return width * height;
  }
}
class Main {
  public static void main(String[] args) {
   Shape rect = new Rectangle(10, 5);
    System.out.println("Area of rectangle: " + rect.area());
 }
}
